! ----- Font Definitions -----
#define Fontname  xft:Tamsyn
#define Fontsize  18

URxvt*font:             Fontname:regular:pixelsize=Fontsize
URxvt*imFont:           Fontname:regular:size=Fontsize
URxvt*boldFont:         Fontname:bold:pixelsize=Fontsize
URxvt*italicFont:       Fontname:italic:autohint=true:pixelsize=Fontsize
URxvt*boldItalicFont:   Fontname:bold:italic:autohint=true:pixelsize=Fontsize

URxvt*scrollBar: false
URxvt*lineSpace: 2
URxvt.internalBorder: 15
URxvt.iso14755: False
URxvt.perl-ext-common: default,clipboard

URxvt.inputMethod: ibus
URxvt.preeditType: OverTheSpot

! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #1c2023, #95aec7, #1c2023
rofi.color-normal: #1c2023, #c7ccd1, #1c2023, #95aec7, #1c2023
rofi.color-active: #1c2023, #ae95c7, #1c2023, #8fa1b3, #1c2023
rofi.color-urgent: #1c2023, #c795ae, #1c2023, #dc322f, #ebcb8b