! ------- Colors -------
! special
*.foreground:   #ececec
*.background:   #2c3941
*.cursorcolor:  #ececec

! black
*.color0:       #2c3941
*.color8:       #2c3941

! red
*.color1:       #865f5b
*.color9:       #865f5b

! green
*.color2:       #ececec
*.color10:      #ececec

! yellow
*.color3:       #b1a990
*.color11:      #b1a990

! blue
*.color4:       #6a8e95
*.color12:      #6a8e95

! magenta
*.color5:       #b18a73
*.color13:      #b18a73

! cyan
*.color6:       #88b2ac
*.color14:      #88b2ac

! white
*.color7:       #ececec
*.color15:      #ececec


! ----- Font Definitions -----
#define Fontname  xft:Inconsolata-g
#define Fontsize  16

URxvt*font:             Fontname:regular:pixelsize=Fontsize
URxvt*imFont:           Fontname:regular:size=Fontsize
URxvt*boldFont:         Fontname:bold:pixelsize=Fontsize
URxvt*italicFont:       Fontname:italic:autohint=true:pixelsize=Fontsize
URxvt*boldItalicFont:   Fontname:bold:italic:autohint=true:pixelsize=Fontsize

URxvt*scrollBar: false
URxvt*lineSpace: 2
URxvt.internalBorder: 15
URxvt.iso14755: False
URxvt.perl-ext-common: default,clipboard

URxvt.inputMethod: ibus
URxvt.preeditType: OverTheSpot

! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #2c3941, #88b2ac, #2c3941
rofi.color-normal: #2c3941, #ececec, #2c3941, #88b2ac, #2c3941
rofi.color-active: #b1a990, #2c3941, #2c3941, #66907d, #2c3941
rofi.color-urgent: #b1a990, #dc322f, #2c3941, #dc322f, #b1a990
