! X colors.
! Generated by 'wal'
URxvt*foreground:   #eae4da
XTerm*foreground:   #eae4da
UXTerm*foreground:  #eae4da
URxvt*background:   [100]#463f51
XTerm*background:   #463f51
UXTerm*background:  #463f51
URxvt*cursorColor:  #eae4da
XTerm*cursorColor:  #eae4da
UXTerm*cursorColor: #eae4da
URxvt*borderColor:  [100]#463f51

! Colors 0-15.
*.color0: #463f51
*color0:  #463f51
*.color1: #E0A19B
*color1:  #E0A19B
*.color2: #D3AFAC
*color2:  #D3AFAC
*.color3: #D7CAB7
*color3:  #D7CAB7
*.color4: #E6CFB4
*color4:  #E6CFB4
*.color5: #B8BDC2
*color5:  #B8BDC2
*.color6: #D7D3C4
*color6:  #D7D3C4
*.color7: #eae4da
*color7:  #eae4da
*.color8: #a39f98
*color8:  #a39f98
*.color9: #E0A19B
*color9:  #E0A19B
*.color10: #D3AFAC
*color10:  #D3AFAC
*.color11: #D7CAB7
*color11:  #D7CAB7
*.color12: #E6CFB4
*color12:  #E6CFB4
*.color13: #B8BDC2
*color13:  #B8BDC2
*.color14: #D7D3C4
*color14:  #D7D3C4
*.color15: #eae4da
*color15:  #eae4da

! Black color that will not be affected by bold highlighting.
*.color66: #463f51
*color66:  #463f51

! Xclock colors.
XClock*foreground: #eae4da
XClock*background: #463f51
XClock*majorColor:  rgba:ea/e4/da/ff
XClock*minorColor:  rgba:ea/e4/da/ff
XClock*hourColor:   rgba:ea/e4/da/ff
XClock*minuteColor: rgba:ea/e4/da/ff
XClock*secondColor: rgba:ea/e4/da/ff

! Set depth to make transparency work.
URxvt*depth: 32


! ----- DPI Settings -----
Xft.dpi: 168
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb1

! ----- Font Definitions -----
#define Fontname  xft:Source Code Pro
#define Fontsize  18

URxvt*font:             Fontname:regular:pixelsize=Fontsize
URxvt*imFont:           Fontname:regular:size=Fontsize
URxvt*boldFont:         Fontname:bold:pixelsize=Fontsize
URxvt*italicFont:       Fontname:italic:autohint=true:pixelsize=Fontsize
URxvt*boldItalicFont:   Fontname:bold:italic:autohint=true:pixelsize=Fontsize

URxvt*scrollBar: false
URxvt*lineSpace: 2
URxvt.internalBorder: 15
URxvt.iso14755: False
URxvt.perl-ext-common: default,clipboard

URxvt.inputMethod: ibus
URxvt.preeditType: OverTheSpot

! ----- Rofi -----
rofi.color-window: #463f51, #463f51, #463f51
rofi.color-normal: #463f51, #eae4da, #463f51, #D3AFAC, #463f51
rofi.color-active: #463f51, #eae4da, #463f51, #D3AFAC, #463f51
rofi.color-urgent: #463f51, #E0A19B, #463f51, #E0A19B, #eae4da
